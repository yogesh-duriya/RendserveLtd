<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="rendserve.rendserveltd.fragment.PhotoFragement"
    android:background="@color/black_color">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp">
    <ImageView
        android:id="@+id/doc_img_2"
        android:layout_width="85dp"
        android:layout_height="110dp"
        android:layout_alignTop="@+id/doc_img_1"
        android:layout_centerHorizontal="true"
        android:padding="10dp"
        android:background="@drawable/imagebackgroundne"
        android:src="@drawable/ic_attachment_file"
        app:layout_constraintEnd_toEndOf="@+id/textView13"
        app:layout_constraintHorizontal_bias="0.452"
        app:layout_constraintStart_toStartOf="@+id/textView13"
        app:layout_constraintTop_toBottomOf="@+id/textView13" />

        <ImageView
            android:id="@+id/doc_img_3"
            android:layout_width="85dp"
            android:layout_height="110dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:layout_marginEnd="24dp"
            android:layout_marginRight="24dp"
            android:background="@drawable/imagebackgroundne"
            android:padding="10dp"
            android:src="@drawable/ic_attachment_file"
            app:layout_constraintEnd_toEndOf="@+id/textView13"
            app:layout_constraintHorizontal_bias="0.452"
            app:layout_constraintStart_toStartOf="@+id/textView13"
            app:layout_constraintTop_toBottomOf="@+id/textView13" />

        <ImageView
            android:id="@+id/doc_img_1"
            android:layout_width="85dp"
            android:layout_height="110dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="30dp"
            android:padding="10dp"
            android:layout_marginStart="30dp"
            android:background="@drawable/imagebackgroundne"
            android:src="@drawable/ic_attachment_file"
            app:layout_constraintEnd_toEndOf="@+id/textView13"
            app:layout_constraintHorizontal_bias="0.452"
            app:layout_constraintStart_toStartOf="@+id/textView13"
            app:layout_constraintTop_toBottomOf="@+id/textView13" />
    </RelativeLayout>
</RelativeLayout>

